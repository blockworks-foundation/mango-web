    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.0",
