    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "engines": {
    "node": ">=18.x"
  },
  "lint-staged": {
    "*.@(ts|tsx|js|jsx)": [
      "yarn format"
    ]
  },
  "dependencies": {
