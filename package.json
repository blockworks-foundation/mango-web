    "decimal.js": "^10.4.3",
    "gsap": "^3.12.2",
    "i18next": "^23.7.6",
    "immer": "^10.0.3",
    "next": "^14.0.3",
    "next-i18next": "^15.0.0",
    "next-plausible": "^3.11.3",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-numbers": "^3.0.8",
    "react-i18next": "^13.5.0",
    "recharts": "^2.10.0"
