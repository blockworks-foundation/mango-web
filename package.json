  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "tc": "yarn type-check --watch",
    "type-check": "tsc --pretty --noEmit",
